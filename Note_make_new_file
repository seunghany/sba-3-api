-- 여기서 부터 터미널
touch app.py // (create app.py)

code app.py // (Enter app.py)

-- 여기서 부터 코드
from flask import flask

app = Flask(__name__)

@app.route('/')
def index():
    return "Hello World"


if __name__ == "__main__":
    app.run()

-- 다시 여기서 부터 터미널
python app.py